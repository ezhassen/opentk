<Project>
	<PropertyGroup>
		<Authors>The OpenTK team.</Authors>
		<PackageIconUrl>https://raw.githubusercontent.com/opentk/opentk/master/docs/files/img/logo.png</PackageIconUrl>
		<!--No image !! cannot pack-->
		<!--<PackageIcon>images/opentk-blue-hexagon.png</PackageIcon>-->
		<PackageLicenseUrl>https://licenses.nuget.org/MIT</PackageLicenseUrl>
		<PackageReleaseNotes>Initial 4.9.5 release for .net9.0.</PackageReleaseNotes>
		<PackageTags>opentk;opengl;cross-platform;openal;csharp;fsharp;vb;dotnet;mono;graphics;game;scientific;science;3d;2d;math;input;gamepad;joystick</PackageTags>
		<PackageProjectUrl>https://github.com/opentk/opentk</PackageProjectUrl>
		<PackageOutputPath>$(OpenTKSolutionRoot)\nuget</PackageOutputPath>
		<IncludeSymbols>true</IncludeSymbols>
		<IncludeSource>true</IncludeSource>
		<!-- The build system will override this version with the actual version. See build.fs. -->
		<!-- - Noggin_bops 2024-12-10 -->
		<Version>4.9.5</Version>
	</PropertyGroup>
	<Import Project="common.props" Condition="!$(HasCommonProperties)"/>
</Project>
